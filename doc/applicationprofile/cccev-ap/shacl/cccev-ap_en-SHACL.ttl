@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix shacl: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en> <https://duet.dev-vlaanderen.be/shacl/cccev-ap_enshapes> <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#StandardShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#CriterionShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#ReferenceFrameworkShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#AgentShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#EvidenceTypeShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#EvidenceShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#EvidenceTypeListShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#RequirementShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#ConceptShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#ConstraintShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#evidence-typesShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#DatasetShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#DistributionShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#custom-data-typeShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#dataset-typeShape>,
    <https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#InformationRequirementShape> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#AgentShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#Agent> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#ConceptShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:definition "An unambiguous reference to RequirementConcept"@en;
    shacl:name "identifier"@en;
    shacl:path <http://fixme.com#cbc:id>
  ], [
    shacl:definition "A short explanation about the nature, attributes, uses or any other additional information that helps clarify the understanding of the RequirementConcept being instantiated."@en;
    shacl:name "description"@en;
    shacl:path <http://fixme.com#cbc:description>
  ], [
    shacl:definition "a classification of the RequirementConcept"@en;
    shacl:maxCount 1;
    shacl:name "type"@en;
    shacl:path <http://fixme.com#cbc:type>
  ], [
    shacl:definition "A name to identify in common language the RequirementConcept"@en;
    shacl:name "name"@en;
    shacl:path <http://fixme.com#cbc:name>
  ], [
    shacl:class <http://fixme.com#Constraint>;
    shacl:definition "The limitations imposed to a requirement concept by the requirement that requests information about the requirement concept."@en;
    shacl:name "has Constraint"@en;
    shacl:path <http://fixme.com#cccev:hasConstraint>
  ];
  shacl:targetClass <http://fixme.com#Concept> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#ConstraintShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#Constraint> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#CriterionShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:definition "The explanation of how the weighting of a Criterion is to be used at evaluation time."@en;
    shacl:name "weighting consideration description"@en;
    shacl:path <http://fixme.com#cccev:weightingConsiderationDescription>
  ], [
    shacl:definition "Additional parameter used to adjust the evaluation of the criterion composed of multiple sub-criteria."@en;
    shacl:name "bias"@en;
    shacl:path <http://fixme.com#cccev:bias>
  ], [
    shacl:definition "Relative importance assigned to a given criterion."@en;
    shacl:maxCount 1;
    shacl:name "weight"@en;
    shacl:path <http://fixme.com#cccev:weight>
  ], [
    shacl:definition "The numeric form used to weight."@en;
    shacl:maxCount 1;
    shacl:name "weighting type"@en;
    shacl:path <http://fixme.com#cccev:weightingType>
  ];
  shacl:targetClass <http://fixme.com#Criterion> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#DatasetShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:maxCount 1;
    shacl:name "issued"@en;
    shacl:path <http://fixme.com#dct:issued>
  ], [
    shacl:maxCount 1;
    shacl:name "type"@en;
    shacl:path <http://fixme.com#dct:type>
  ], [
    shacl:minCount 1;
    shacl:name "description"@en;
    shacl:path <http://fixme.com#dct:description>
  ], [
    shacl:name "identifier"@en;
    shacl:path <http://fixme.com#dct:identifier>
  ], [
    shacl:class <http://fixme.com#Distribution>;
    shacl:path <http://fixme.com#dcat:distribution>
  ], [
    shacl:minCount 1;
    shacl:name "title"@en;
    shacl:path <http://fixme.com#dct:title>
  ], [
    shacl:name "modified"@en;
    shacl:path <http://fixme.com#dct:modified>
  ];
  shacl:targetClass <http://fixme.com#Dataset> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#DistributionShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:maxCount 1;
    shacl:path <http://fixme.com#dcat:downloadURL>
  ], [
    shacl:datatype xsd:date;
    shacl:maxCount 1;
    shacl:path <http://fixme.com#dct:issued>
  ];
  shacl:targetClass <http://fixme.com#Distribution> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#EvidenceShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:definition "The security classification assigned to an Evidence or, in general, to a content."@en;
    shacl:maxCount 1;
    shacl:name "confidentiality level type"@en;
    shacl:path <http://fixme.com#cccev:confidentialityLevelType>
  ], [
    shacl:class <http://fixme.com#EvidenceType>;
    shacl:definition "The kind of evidence"@en;
    shacl:name "is Conformant"@en;
    shacl:path <http://fixme.com#cccev:isConformantTo>
  ], [
    shacl:class <http://fixme.com#Requirement>;
    shacl:path <http://fixme.com#cccev:supports>
  ], [
    shacl:class <http://fixme.com#Concept>;
    shacl:path <http://fixme.com#cccev:supports>
  ];
  shacl:targetClass <http://fixme.com#Evidence> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#EvidenceTypeListShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class <http://fixme.com#EvidenceType>;
    shacl:name "specifies evidence type"@en;
    shacl:path <http://fixme.com#cccev:specifiesEvidenceType>
  ], [
    shacl:definition "A short explanation about the nature, attributes, uses or any other additional information that helps clarify the understanding of the Evidence Type  being instantiated."@en;
    shacl:name "description"@en;
    shacl:path <http://fixme.com#cbc:description>
  ], [
    shacl:definition "A name to identify in common language the Evidence Type List"@en;
    shacl:name "name"@en;
    shacl:path <http://fixme.com#cbc:name>
  ], [
    shacl:definition "An unambiguous reference to an Evidence Type List"@en;
    shacl:maxCount 1;
    shacl:name "identifier"@en;
    shacl:path <http://fixme.com#cbc:id>
  ];
  shacl:targetClass <http://fixme.com#EvidenceTypeList> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#EvidenceTypeShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:definition "A classification code use to specify the machine-readability of a content."@en;
    shacl:maxCount 1;
    shacl:name "dataset type"@en;
    shacl:path <http://fixme.com#cccev:datasetType>
  ], [
    shacl:class <http://fixme.com#EvidenceTypeList>;
    shacl:minCount 1;
    shacl:name "is specified in"@en;
    shacl:path <http://fixme.com#cccev:isSpecifiedIn>
  ], [
    shacl:definition "A classification code to specify the layout and content expected for an evidence."@en;
    shacl:maxCount 1;
    shacl:name "evidence type classification"@en;
    shacl:path <http://fixme.com#cccev:evidenceTypeClassification>
  ];
  shacl:targetClass <http://fixme.com#EvidenceType> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#InformationRequirementShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#InformationRequirement> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#ReferenceFrameworkShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#ReferenceFramework> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#RequirementShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class <http://fixme.com#Concept>;
    shacl:name "has concept"@en;
    shacl:path <http://fixme.com#cccev:hasConcept>
  ], [
    shacl:name "identifier"@en;
    shacl:path <http://fixme.com#cbc:id>
  ], [
    shacl:class <http://fixme.com#ReferenceFramework>;
    shacl:name "is derived from"@en;
    shacl:path <http://fixme.com#cccev:isDerivedFrom>
  ], [
    shacl:maxCount 1;
    shacl:name "type"@en;
    shacl:path <http://fixme.com#cbc:type>
  ], [
    shacl:class <http://fixme.com#Evidence>;
    shacl:name "has supporting evidence"@en;
    shacl:path <http://fixme.com#cccev:hasSupportingEvidence>
  ], [
    shacl:class <http://fixme.com#EvidenceTypeList>;
    shacl:name "has evidence type combination"@en;
    shacl:path <http://fixme.com#cccev:hasEvidenceTypeCombinationList>
  ], [
    shacl:class <http://fixme.com#Requirement>;
    shacl:name "is requirement of"@en;
    shacl:path <http://fixme.com#cccev:isRequirementOf>
  ], [
    shacl:name "description"@en;
    shacl:path <http://fixme.com#cbc:description>
  ], [
    shacl:class <http://fixme.com#Requirement>;
    shacl:name "has requirement"@en;
    shacl:path <http://fixme.com#cccev:hasRequirement>
  ], [
    shacl:name "name"@en;
    shacl:path <http://fixme.com#cbc:name>
  ];
  shacl:targetClass <http://fixme.com#Requirement> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#StandardShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#Standard> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#custom-data-typeShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#custom-data-type> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#dataset-typeShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#dataset-type> .

<https://duet.dev-vlaanderen.be/shacl/cccev-ap_en#evidence-typesShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass <http://fixme.com#evidence-types> .
