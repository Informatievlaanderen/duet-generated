<https://duet.dev-vlaanderen.be/shacl/sdgtest2-ap_en#CourseResultShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:definition """ 	The number of ECTS credits for the course, expressing the volume of learning based on the defined learning outcomes and their associated workload."""@en;
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "course credits amount"@en;
    sh:path <https://sdg.semic.eucourseCreditsAmount>
  ], [
    sh:class skos:Concept;
    sh:definition "The discipline or subject area of a course."@en;
    sh:name "course field of study"@en;
    sh:path <https://sdg.semic.eucourseFieldOfStudy>
  ], [
    sh:class skos:Concept;
    sh:definition "Main language in which the course was taught."@en;
    sh:maxCount 1;
    sh:name "course language"@en;
    sh:path <https://sdg.semic.eucourseLanguage>
  ], [
    sh:definition "Period of the academic year when the course took place, e.g. first semester, second trimester, etc."@en;
    sh:maxCount 1;
    sh:name "course academic period"@en;
    sh:path <https://sdg.semic.eucourseAcademicPeriod>
  ], [
    sh:datatype xsd:date;
    sh:definition """ 	The annual period of sessions of an educational programme usually beginning in September and ending in June."""@en;
    sh:maxCount 1;
    sh:name "course academic period"@en;
    sh:path <https://sdg.semic.eucourseAcademicYear>
  ], [
    sh:class <https://sdg.semic.euGrade>;
