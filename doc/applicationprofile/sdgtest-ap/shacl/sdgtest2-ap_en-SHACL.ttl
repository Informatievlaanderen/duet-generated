    sh:path <https://sdg.semic.euthesisTitle>
  ], [
    sh:definition "Details of any rights to practise, or professional title, accorded to the holder of the qualification, in accordance with national legislation or requirements by a competent authority."@en;
    sh:name "access to regulated profession"@en;
    sh:path <https://sdg.semic.euaccessToRegulatedProfession>
  ];
  sh:targetClass <https://sdg.semic.euTertiaryEducationDiplomaEvidence> .

