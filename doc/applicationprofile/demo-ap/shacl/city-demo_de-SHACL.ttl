@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schemas: <https://schema.org/> .
@prefix shacl: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://duet.dev-vlaanderen.be/shacl/city-demo_de> <https://duet.dev-vlaanderen.be/shacl/city-demo_deshapes> <https://duet.dev-vlaanderen.be/shacl/city-demo_de#CityShape>,
    <https://duet.dev-vlaanderen.be/shacl/city-demo_de#AddressShape> .

<https://duet.dev-vlaanderen.be/shacl/city-demo_de#AddressShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype xsd:string;
    shacl:definition "Der Name der öffentlichen Straße in einer Stadt oder einem Dorf entlang der Adresse befindet sich"@de;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "strassenname"@de;
    shacl:path <http://www.w3.org/ns/locn#thoroughfare>
  ], [
    shacl:datatype xsd:string;
    shacl:definition "Eine eindeutige Angabe, normalerweise eine Zahl, in einer Straße, die Orten zugeordnet ist, meistens Häusern oder Einheiten innerhalb von Häusern"@de;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "hausnummer"@de;
    shacl:path <http://www.w3.org/ns/locn#poBox>
  ], [
    shacl:class schemas:City;
    shacl:definition "Die Stadt in der sich die Adressen befinden."@de;
    shacl:maxCount 1;
    shacl:name "stadt"@de;
    shacl:path <http://www.w3.org/ns/locn#addressArea>
  ], [
    shacl:datatype xsd:string;
    shacl:definition "Eine Gruppe von Zahlen oder Buchstaben und Zahlen, die einer Postanschrift hinzugefügt werden, um das Sortieren von Post zu erleichtern."@de;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "postleitzahl"@de;
    shacl:path <http://www.w3.org/ns/locn#postCode>
  ];
  shacl:targetClass <http://www.w3.org/ns/locn#Address> .

<https://duet.dev-vlaanderen.be/shacl/city-demo_de#CityShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype xsd:string;
    shacl:definition "der Name der Stadt"@de;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "name"@de;
    shacl:path <http://www.w3.org/ns/locn#geographicName>
  ];
  shacl:targetClass schemas:City .
